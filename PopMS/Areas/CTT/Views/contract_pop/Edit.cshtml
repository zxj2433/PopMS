@model PopMS.ViewModel.CTT.contract_popVMs.contract_popVM
@inject IStringLocalizer<WalkingTec.Mvvm.Core.Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:combobox field="Entity.ContractID" items="AllContracts" />
        <wt:combobox field="Entity.PopID" items="AllPops" />
        <wt:textbox field="Entity.UnitPack" />
        <wt:textbox field="Entity.Cnt" />
        <wt:textbox field="Entity.Price" />
    </wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
