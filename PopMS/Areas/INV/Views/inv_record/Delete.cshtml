@model PopMS.ViewModel.INV.inv_recordVMs.inv_recordVM
@inject IStringLocalizer<WalkingTec.Mvvm.Core.Program> Localizer;

<wt:form vm="@Model">
	<wt:quote>@Model.Localizer["DeleteConfirm"]</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.Inv.Location.Location" />
<wt:display field="Entity.Type" />
<wt:display field="Entity.ToLoc.Location" />
<wt:display field="Entity.Qty" />
<wt:display field="Entity.UserName" />
<wt:display field="Entity.UpdateTime" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>
