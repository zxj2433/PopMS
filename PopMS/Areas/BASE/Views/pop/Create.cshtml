@model PopMS.ViewModel.BASE.popVMs.popVM
@inject IStringLocalizer<WalkingTec.Mvvm.Core.Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.One">
    <wt:combobox field="Entity.GroupID" items="AllGroups" />
<wt:textbox field="Entity.PopNo" />
